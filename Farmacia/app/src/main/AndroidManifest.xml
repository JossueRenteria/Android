<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uan.jossuesl.farmacia" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BuscarActivity"
            android:label="@string/title_activity_buscar" >
        </activity>
        <activity
            android:name=".EliminarActivity"
            android:label="@string/title_activity_eliminar" >
        </activity>
        <activity
            android:name=".InsertarActivity"
            android:label="@string/title_activity_insertar" >
        </activity>
    </application>

</manifest>
